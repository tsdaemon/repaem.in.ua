@model IEnumerable<aspdev.repaem.ViewModel.Repetition>

@{
    ViewBag.Title = "repaem.in.ua - Ваши репетиции";
}

<div class="narrow-page">
    @if(Model.Count() == 0) { 
        <h2>У вас нет репетиций!</h2> 
    }
    else { 
        <h2>Ваши репетиции</h2>
        @Html.DisplayForModel()
    }
</div>